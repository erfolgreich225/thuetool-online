{
  "name": "thuetool-online",
  "version": "1.0.0",
  "description": "Full stack application for renting Unlocktool accounts",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/index.js",
    "client": "react-scripts start",
    "build": "npm run build-server && npm run build-client",
    "build-server": "node backend/build.js",
    "build-client": "react-scripts build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22"
  }
}
